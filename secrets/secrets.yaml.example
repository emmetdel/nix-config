# Example secrets file - DO NOT COMMIT THIS FILE IF IT CONTAINS REAL SECRETS
# Copy this to secrets.yaml and then encrypt with: sops -e -i secrets.yaml

ssh:
  # Personal SSH key (private)
  personal_id_ed25519: |
    -----BEGIN OPENSSH PRIVATE KEY-----
    b3BlbnNzaC1rZXktdjEAAAAABG5vbmUAAAAEbm9uZQAAAAAAAAABAAAAMwAAAAtzc2gtZW
    ...paste your full private key here...
    -----END OPENSSH PRIVATE KEY-----

  # Personal SSH key (public)
  personal_id_ed25519_pub: ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx your@email.com

  # Work SSH key (private)
  work_id_ed25519: |
    -----BEGIN OPENSSH PRIVATE KEY-----
    b3BlbnNzaC1rZXktdjEAAAAABG5vbmUAAAAEbm9uZQAAAAAAAAABAAAAMwAAAAtzc2gtZW
    ...paste your full work private key here...
    -----END OPENSSH PRIVATE KEY-----

  # Work SSH key (public)
  work_id_ed25519_pub: ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx work@email.com

# You can add other secrets here too:
# github_token: ghp_xxxxxxxxxxxxxxxxxxxxx
# api_keys:
#   openai: sk-xxxxxxxxxxxxxxxxxxxxx
#   anthropic: sk-ant-xxxxxxxxxxxxxxxxxxxxx



